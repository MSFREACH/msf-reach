{
  "name": "msf-reach",
  "version": "1.1.0",
  "description": "Web server for CogniCity MSF",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "mocha": "nyc mocha --timeout 5000 --compilers js:babel-core/register src/test/testMSFREACHServer.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "eslint src && eslint public && npm run-script mocha"
  },
  "nyc": {
    "exclude": [
      "**/test/**",
      "build"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/MSFREACH/msf-reach",
  "issues": "https://github.com/MSFREACH/msf-reach/issues",
  "author": "Matthew Berryman, Tomas Holderness",
  "license": "GPL-3.0",
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "apicache": "^1.2.1",
    "aws-sdk": "^2.238.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "celebrate": "^7.0.6",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cors-anywhere": "^0.4.1",
    "dbgeo": "^1.1.0",
    "dms-conversion": "3.1.1-alpha.1",
    "dotenv": "^5.0.1",
    "express": "git+https://git@github.com/AcrossTheCloud/express.git",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "joi": "^13.3.0",
    "joi-date-extensions": "^1.2.0",
    "jwk-to-pem": "^2.0.0",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nocache": "^2.0.0",
    "nodemailer": "^4.6.4",
    "passport": "0.4.0",
    "passport-azure-ad": "^3.0.12",
    "pg-promise": "^8.4.4",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "response-time": "^2.3.2",
    "save": "^2.3.2",
    "twitter": "^1.7.1",
    "winston": "^2.4.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "it-each": "^0.3.1",
    "jsonwebtoken": "^8.2.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "nyc": "^11.7.3",
    "unit.js": "^2.0.0"
  },
  "engines": {
    "node": ">=6.9.1"
  }
}
