{
  "name": "msf-reach",
  "version": "0.0.1",
  "description": "Web server for CogniCity MSF",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "mocha": "nyc mocha --compilers js:babel-core/register src/test/testCognicityServer.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "eslint src && npm run-script mocha"
  },
  "nyc": {
    "exclude": [
      "**/test/**",
      "build"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/usergroupcoop/msf-reach",
  "issues": "https://github.com/usergroupcoop/msf-reach/issues",
  "author": "Matthew Berryman, Tomas Holderness",
  "license": "GPL-3.0",
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "apicache": "^0.9.0",
    "aws-sdk": "^2.101.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "celebrate": "^4.0.3",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "dbgeo": "^1.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "joi": "^10.6.0",
    "jwk-to-pem": "^1.2.6",
    "morgan": "^1.8.2",
    "pg-promise": "^6.5.1",
    "request": "^2.81.0",
    "response-time": "^2.3.2",
    "shortid": "^2.2.8",
    "twitter": "^1.7.1",
    "winston": "^2.3.1",
    "xml2js": "^0.4.18"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.5.0",
    "it-each": "^0.3.1",
    "jsonwebtoken": "^7.4.3",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "unit.js": "^2.0.0"
  },
  "engines": {
    "node": ">=6.9.1"
  }
}
