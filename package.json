{
  "name": "msf-reach",
  "version": "1.1.0",
  "description": "Web server for CogniCity MSF",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src \"",
    "build": "babel src -s -D -d dist",
    "buildvue": "node app/build/build.js",
    "start": "node dist",
    "prestart": "npm run -s build",
    "mocha": "nyc mocha --timeout 30000 --compilers js:babel-core/register src/test/testMSFREACHServer.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "eslint src && eslint public && npm run-script mocha"
  },
  "nyc": {
    "exclude": [
      "**/test/**",
      "build"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/MSFREACH/msf-reach",
  "issues": "https://github.com/MSFREACH/msf-reach/issues",
  "author": "Matthew Berryman, Tomas Holderness",
  "license": "GPL-3.0",
  "dependencies": {
    "@esri/arcgis-to-geojson-utils": "^1.3.0",
    "@mdi/font": "^2.8.94",
    "ajv": "^6.5.3",
    "api-error-handler": "^1.0.0",
    "apicache": "^1.2.3",
    "aws-sdk": "^2.303.0",
    "axios": "^0.18.0",
    "babel-loader": "^7.1.5",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "bootstrap-datetimepicker-npm": "^4.17.37-npm",
    "celebrate": "^8.0.2",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cors-anywhere": "^0.4.1",
    "dbgeo": "^1.1.0",
    "dms-conversion": "3.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "file-loader": "^2.0.0",
