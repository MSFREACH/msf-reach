{
  "name": "cognicity_msf-server",
  "version": "0.0.1",
  "description": "Data Server for CogniCity MSF",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "mocha": "nyc mocha --compilers js:babel-core/register src/test/testCognicityServer.js",
    "test": "eslint src && npm run-script mocha"
  },
  "nyc": {
  "exclude": [
    "**/test/**",
    "build"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/usergroupcoop/cognicity_msf-server",
  "issues": "https://github.com/usergroupcoop/cognicity_msf-server/issues",
  "author": "Matthew Berryman, Tomas Holderness",
  "license": "GPL-3.0",
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "apicache": "^0.8.4",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "celebrate": "^4.0.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dbgeo": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "joi": "^10.2.2",
    "morgan": "^1.8.1",
    "pg-promise": "^5.7.1",
    "response-time": "^2.3.2",
    "shortid": "^2.2.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.15.0",
    "it-each": "^0.3.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "unit.js": "^2.0.0"
  },
  "engines": {
    "node": ">=6.9.1"
  }
}
